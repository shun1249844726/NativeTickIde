+Blockly.JavaScript['text_getSubstring'] = function(block) {
+  // Get substring.
+  var text = Blockly.JavaScript.valueToCode(block, 'STRING',
+      Blockly.JavaScript.ORDER_FUNCTION_CALL) || '\'\'';
